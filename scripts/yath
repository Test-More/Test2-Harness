#!/usr/bin/perl

# Avoid contaminating the 'main' namespace, and the current scope (which could
# be re-rused by goto_alt_script()!)
# Do not use strict or warnings. Keep this file as tiny as possible

BEGIN {
    return if $^C;
    require App::Yath::Script;
    App::Yath::Script::goto_alt_script('yath', __FILE__, './scripts/yath');
}

exit(App::Yath::Script::run(__FILE__, \@ARGV));
